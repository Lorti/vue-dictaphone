{
  "name": "vue-dictaphone",
  "description": "Vue.js dictaphone component to record audio from the user",
  "version": "2.0.0",
  "author": {
    "name": "Manuel Wieser",
    "email": "office@manuelwieser.com",
    "url": "https://manu.ninja/"
  },
  "main": "dist/vue-dictaphone.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lorti/vue-dictaphone.git"
  },
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve src/example/main.js",
    "build": "vue-cli-service build --target lib src/index.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^4.3.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "not dead"
  ]
}